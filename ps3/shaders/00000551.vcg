void main(
  float4 aPosition : POSITION
, float2 aTexCoord : TEXCOORD0
, float3 aInput1 : COLOR0
, float3 aInput2 : COLOR1
, float4 out vPosition : POSITION
, float2 out vTexCoord : TEXCOORD0
, float3 out vInput1 : COLOR0
, float3 out vInput2 : COLOR1
) {
vTexCoord = aTexCoord;
vInput1 = aInput1;
vInput2 = aInput2;
vPosition = aPosition.xyzw;
}

